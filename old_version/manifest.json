{
    "manifest_version":2,
  
    "name": "Old version Glossary Tool",
    "description": "This extension will define words on double click.",
    "version": "1.0",
  
   
    "content_scripts":[
      {
        "matches":[
          "<all_urls>"
        ],
        "js": ["content.js"]
      }
    ],
    "background":{
        "scripts": ["background.js"]

    },

    "browser_action": {
        "default_popup": "index.html",
        "default_title": "glossary tool"
    }
  }